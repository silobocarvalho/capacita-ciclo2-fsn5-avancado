generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno{
  id Int @id @default( autoincrement() )
  nome String
  matricula String
  turma String
}

model Professor{
  id Int @id @default( autoincrement() )
  nome String
  email String
  turma String
}
